@model IEnumerable<NovelWeb_SS5.Models.Chuong>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ChuongTrongDetails_1</title>
</head>
<body>

    <table class="table">
        <tr>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.soChuong)
                </td>
                <td>
                    <a href="@Url.Action("ChiTietChuong",new { id = item.idChuong})">@Html.DisplayFor(modelItem => item.tenChuong)</a>
                </td>
            </tr>
        }
    </table>
</body>
</html>